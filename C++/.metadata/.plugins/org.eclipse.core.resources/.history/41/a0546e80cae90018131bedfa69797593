#include <iostream>
using namespace std;

int main() {
	int vector1[7] = {4, 7, 2, 8, 1, 3, 0};
	int vector2[7];
	for (int a = 0; a < 7; a++)
		cout << vector2[a] << ' ';
	for (int b = 1; b < 7; b++)
		cout << vector2[b] << ' ';
	for(int c = 2; c < 7; c++)
		cout << vector2[c] << ' ';
	for(int d = 3; d < 7; d++)
		cout << vector2[d] << ' ';
	for(int e = 4; e < 7; e++)
		cout << vector2[e] << ' ';
	for(int f = 5; f < 7; f++)
		cout << vector2[f] << ' ';
	for(int i = 6; i < 7; i++)
		cout << vector2[i] << ' ';
	cout << endl;
	return 0;
}
