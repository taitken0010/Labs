#include <iostream>
using namespace std;

int main() {
	int vector1[7] = {4, 7, 2, 8, 1, 3, 0};
	int vector2[7];
	int a;
	switch(a){
		case 0 : for (int b=0; b<7; b++)
			cout << vector2[b] << ' ';
		break;

		case 1 : for (int c=1; c<7; c++)
			cout << vector2[c] << ' ';
		break;

		case 2 : for (int d=2; d<7; d++)
			cout << vector2[d] << ' ';
		break;

		case 3 : for (int e=3; e<7; e++)
			cout << vector2[e] << ' ';
		break;

		case 4 : for (int f=4; f<7; f++)
			cout << vector2[f] << ' ';
		break;

		case 5 : for (int g=5; g<7; g++)
			cout << vector2[g] << ' ';
		break;

		case 6 : for (int h=6; h<7; g++)
			cout << vector2[h] << ' ';
		break;

	}
	cout << endl;
	return 0;
}
